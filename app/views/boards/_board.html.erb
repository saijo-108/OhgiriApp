<div class="col-sm-12 col-lg-4 mb-3">
  <div id="board-id-<%= board.id %>">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">
          <a href="#">
            <%= board.title %>
          </a>
        </h4>
        <ul class='mr10 float-right list'>
          <li><a href="#">回答する</a></li>
          <li><a href="#">他の人の回答</a></li>
        </ul>
        <ul class="list-inline">
          <li class="list-inline-item">
            <p>出題者:<%= board.user.nick_name %>さん</p>
          <li class="list-inline-item">
            <p>回答者: 5人</p>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>