<% content_for :css do %>
  <%= stylesheet_link_tag 'shared/error' %>
<% end %>
<% if object.errors.any? %>
    <div id="error_messages" class="alert alert-danger">
      <ul class="mb-0">
        <% object.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
<% end %>
